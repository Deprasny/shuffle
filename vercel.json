{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/(.*).(png|jpg|jpeg|gif|webp|svg|ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
